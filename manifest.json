{
  "manifest_version": 3,
  "name": "InstaSave Navigator",
  "version": "1.0",
  "description": "Jump to your first saved Instagram post instantly.",
  "permissions": ["activeTab"],
  "host_permissions": ["https://www.instagram.com/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
